{
  "name": "backend-caleb-car",
  "version": "1.0.0",
  "description": "Vehicle Renting Management System Backend - Caleb",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch ./src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "gen": "drizzle-kit generate",
    "migrate": "tsx src/drizzle/migrate.ts",
    "studio": "drizzle-kit studio",
    "push": "drizzle-kit generate && drizzle-kit push",
    "seed": "tsx src/drizzle/seed.ts",
    "seed:comprehensive": "tsx src/drizzle/comprehensive-seed.ts",
    "seed:vehicles": "tsx src/drizzle/vehicles-focused-seed.ts",
    "seed:vehicles:force": "tsx src/drizzle/vehicles-focused-seed.ts --clear",
    "seed:force": "tsx src/drizzle/comprehensive-seed.ts --clear",
    "seed:production": "tsx src/drizzle/production-seed.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "node",
    "typescript",
    "express",
    "drizzle",
    "postgresql",
    "backend",
    "vehicle",
    "rental"
  ],
  "author": "Caleb Ogeto",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.7.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.3",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.3",
    "rate-limiter-flexible": "^7.1.1",
    "socket.io": "4.8.1",
    "stripe": "^18.4.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-dist": "^5.25.3",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@jest/globals": "^29.6.3",
    "@jest/types": "^29.6.3",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.21",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^29.6.3",
    "drizzle-kit": "^0.31.1",
    "express": "4.18.2",
    "jest": "^29.6.3",
    "only-allow": "^1.2.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "ws": "8.18.3"
  }
}
